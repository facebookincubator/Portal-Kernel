&i2c_4 {
	tmp103b@0x71 {
		compatible = "tmp103";
		reg = <0x71>;
		label = "wifi_temp";
	};

	tmp103c@0x72 {
		compatible = "tmp103";
		reg = <0x72>;
		label = "apq_temp";
	};
};


