/*
 * This is a platform device for the camera status LED that
 * is controlled via TZ.
 */
/ {
	fb_privacy: fb_privacy {
		fb,release_timeout = <0>;
		fb,key_code = <113>;
	};

};

/* Delete led driver */
&spmi_bus {
	qcom,pmi8998@3 {
		/delete-node/ qcom,leds@d000;
	};
};

&i2c_6 {
	/delete-node/ tlc59116@69;
	/delete-node/ tlc59116@6A;
};

/* Delete smb driver */
/delete-node/ &smb138x;
